{
  "name": "doppio-coffee-mcp",
  "version": "1.0.1",
  "description": "MCP server for ordering Doppio coffee",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "doppio-coffee-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "bundle": "esbuild src/index.ts --bundle --platform=node --format=esm --minify --outfile=dist/doppio-coffee-mcp.mjs --banner:js='#!/usr/bin/env node'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ]
}
